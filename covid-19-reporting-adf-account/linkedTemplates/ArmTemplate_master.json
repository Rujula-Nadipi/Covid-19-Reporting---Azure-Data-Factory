{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "covid-19-reporting-adf-account"
		},
		"ls_adls_rujulacovid19datalake_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_adls_rujulacovid19datalake'"
		},
		"ls_azblob_rujulacovid19sa_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_azblob_rujulacovid19sa'"
		},
		"ls_sql_covid_db_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_sql_covid_db'"
		},
		"ls_adls_rujulacovid19datalake_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://rujulacovid19datalake.dfs.core.windows.net/"
		},
		"ls_http_opendata_github_ecdc_europe_eu_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "@{linkedService().sourceBaseURL}"
		},
		"tr_population_execution_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/798864c3-251f-4833-ad75-610c24586043/resourceGroups/covid-reporting-rg/providers/Microsoft.Storage/storageAccounts/rujulacovid19sa"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "covid-19-reporting-adf-account_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_adls_rujulacovid19datalake_accountKey": {
						"value": "[parameters('ls_adls_rujulacovid19datalake_accountKey')]"
					},
					"ls_azblob_rujulacovid19sa_connectionString": {
						"value": "[parameters('ls_azblob_rujulacovid19sa_connectionString')]"
					},
					"ls_sql_covid_db_connectionString": {
						"value": "[parameters('ls_sql_covid_db_connectionString')]"
					},
					"ls_adls_rujulacovid19datalake_properties_typeProperties_url": {
						"value": "[parameters('ls_adls_rujulacovid19datalake_properties_typeProperties_url')]"
					},
					"ls_http_opendata_github_ecdc_europe_eu_properties_typeProperties_url": {
						"value": "[parameters('ls_http_opendata_github_ecdc_europe_eu_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "covid-19-reporting-adf-account_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"tr_population_execution_properties_typeProperties_scope": {
						"value": "[parameters('tr_population_execution_properties_typeProperties_scope')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'covid-19-reporting-adf-account_ArmTemplate_0')]"
			]
		}
	]
}